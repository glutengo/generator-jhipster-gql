{
  "name": "generator-jhipster-gql",
  "version": "0.1.2",
  "description": "GraphQL integration for JHipster",
  "keywords": [
    "yeoman-generator",
    "jhipster-module",
    "jhipster-7"
  ],
  "homepage": "https://github.com/glutengo/generator-jhipster-gql",
  "author": {
    "name": "Markus Glutting",
    "email": "markus.glutting@student.htw-berlin.de",
    "url": "https://glutting.net"
  },
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glutengo/generator-jhipster-gql.git"
  },
  "dependencies": {
    "@babel/core": "^7.17.9",
    "@babel/generator": "^7.17.9",
    "@babel/traverse": "^7.17.9",
    "@babel/types": "^7.17.0",
    "@ts-morph/common": "^0.11.1",
    "chalk": "3.0.0",
    "comment-json": "^4.2.2",
    "generator-jhipster": "7.8.1",
    "generator-jhipster-nodejs": "2.0.0",
    "json5": "^2.2.1",
    "mem-fs": "^2.2.1",
    "mkdirp": "0.5.1",
    "pluralize": "^8.0.0",
    "semver": "7.1.1",
    "shelljs": "0.8.3",
    "ts-morph": "^11.0.3",
    "yeoman-environment": "^3.2.0",
    "yeoman-generator": "4.4.0"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-prettier": "3.1.2",
    "fs-extra": "8.1.0",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "mocha": "6.2.2",
    "prettier": "1.19.1",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "^6.3.0"
  },
  "scripts": {
    "prettier:format": "prettier --write \"{,**/}*.{js,json,md,yml}\"",
    "pretest": "eslint .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "git push && git push --tags && npm publish",
    "release:patch": "npm version patch -a -m \"Update to %s\" && npm run release",
    "release:minor": "npm version minor -a -m \"Update to %s\" && npm run release",
    "release:major": "npm version major -a -m \"Update to %s\" && npm run release",
    "test": "mocha test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,yml,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/glutengo/generator-jhipster-gql/issues"
  }
}
